	<parameter name="idInscripcion" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select 
e.distrito,
e.provincia,
e.departamento,
i.promocion,
e.dni,
CONCAT(e.apellidos,' ',e.nombres) AS nombres,
e.fecha_nacimiento,
